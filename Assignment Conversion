package selfProgramming;

public class AssignmentConversion {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		boolean a = true;
		boolean b = false;
		
		int dollars = 20;
		double money = dollars;
		
		System.out.println("money is " + money);
		
		float kg = (float) 4.55;
		double weight = kg;
		
		System.out.println("kg is " + weight);
		System.out.println(a);
		System.out.println(b);
		System.out.println(money>weight);
		System.out.println(weight<money);
		System.out.println(weight==money);
	}

}

Output:
money is 20.0
kg is 4.550000190734863
true
false
true
true
false
